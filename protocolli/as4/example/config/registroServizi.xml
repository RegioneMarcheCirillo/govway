<registro-servizi xmlns="http://www.openspcoop2.org/core/registry"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.openspcoop2.org/core/registry registroServizi.xsd">


    <accordo-servizio-parte-comune nome="SoapExample" profilo-collaborazione="oneway"
        utilizzo-senza-azione="false" versione="1" service-binding="soap">
	<soggetto-referente tipo="AS4" nome="MinisteroErogatore" />
        <port-type nome="GestioneFileSOAP" profilo-collaborazione="oneway">
            <azione nome="pubblica">
		<protocol-property name="ebmsUserMessageCollaborationInfoAction" value="TC1Leg1" />
	    </azione>
	    <protocol-property name="ebmsUserMessageCollaborationInfoService" value="bdx:noprocess" />
	    <protocol-property name="ebmsUserMessageCollaborationInfoServiceType" value="tc1" />
        </port-type>
    </accordo-servizio-parte-comune>


    <!-- Soggetto Mittente degli esempi -->
    <soggetto tipo="AS4" nome="MinisteroFruitore">

        <connettore tipo="http" nome="PdDMinisteroFruitore">
            <property nome="location" valore="http://localhost:8080/domibus/services/backend"/>
        </connettore>

	<protocol-property name="ebmsUserMessagePartyId" value="urn:oasis:names:tc:ebcore:partyid-type:unregistered:domibus-red" />

    </soggetto>


    <!-- Soggetto Destinatario degli esempi -->
    <soggetto tipo="AS4" nome="MinisteroErogatore" >

        <connettore tipo="http" nome="PdDMinisteroErogatore">
            <property nome="location" valore="http://localhost:8080/domibus/services/backend"/>
        </connettore>

	<protocol-property name="ebmsUserMessagePartyId" value="urn:oasis:names:tc:ebcore:partyid-type:unregistered:domibus-blue" />

        <accordo-servizio-parte-specifica tipo="AS4" nome="GestioneFileSOAP"
            accordo-servizio-parte-comune="AS4/MinisteroErogatore:SoapExample:1"
	    port-type="GestioneFileSOAP">
            <fruitore tipo="AS4" nome="MinisteroFruitore"/>
        </accordo-servizio-parte-specifica>

    </soggetto>

</registro-servizi>
