<?xml version="1.0" encoding="iso-8859-1"?>

<project name="restNonBlockingPush_LocalEnv" basedir=".">

      <!-- root di OpenSPCoop -->
      <property name="openspcoop2" location="../../../../../../"/>
      <property name="openspcoop2.dist" location="${openspcoop2}/core/dist"/>
      
      <property name="required_lib_restNonBlockingPush" location="${openspcoop2}/lib/"/>
      <property name="required_lib" value="${required_lib_restNonBlockingPush}"/>
      <property name="openspcoop2.lib" location="${required_lib_restNonBlockingPush}"/>

      <!-- Build-Options -->
      <property name="buildAsRelease" value="false"/>
      <property name="buildrestNonBlockingPush" value="true"/>
      <property name="buildAPI" value="true"/>

       <!-- Specifica se inserire le informazioni di debug nelle classi -->
      <property name="debug" value="true" />
      <property name="printServerInfo" value="false" />

</project>
