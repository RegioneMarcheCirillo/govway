<?xml version="1.0" encoding="UTF-8"?>
<binding force-classes="true">
  <mapping name="message-context" class="org.openspcoop2.message.context.MessageContext">
    <namespace uri="http://www.openspcoop2.org/message/context" default="elements"/>
    <structure field="transportRequestContext" usage="optional" map-as="org.openspcoop2.message.context.TransportRequestContext"/>
    <structure field="transportResponseContext" usage="optional" map-as="org.openspcoop2.message.context.TransportResponseContext"/>
    <structure field="forcedResponse" usage="optional" map-as="org.openspcoop2.message.context.ForcedResponse"/>
    <structure field="serializedContext" usage="optional" map-as="org.openspcoop2.message.context.SerializedContext"/>
    <structure field="contentTypeParameters" usage="optional" map-as="org.openspcoop2.message.context.ContentTypeParameters"/>
    <structure field="contentLength" usage="optional" map-as="org.openspcoop2.message.context.ContentLength"/>
    <structure field="soap" usage="optional" map-as="org.openspcoop2.message.context.Soap"/>
    <value name="message-type" field="messageType" usage="required" style="attribute"/>
    <value name="message-role" field="messageRole" usage="required" style="attribute"/>
    <value name="protocol" field="protocol" usage="required" style="attribute"/>
  </mapping>
  <mapping name="soap" class="org.openspcoop2.message.context.Soap">
    <namespace uri="http://www.openspcoop2.org/message/context" default="elements"/>
    <value name="soap-action" field="soapAction" usage="optional"/>
  </mapping>
  <mapping name="transport-request-context" class="org.openspcoop2.message.context.TransportRequestContext">
    <namespace uri="http://www.openspcoop2.org/message/context" default="elements"/>
    <structure field="urlParameters" usage="optional" map-as="org.openspcoop2.message.context.UrlParameters"/>
    <structure name="header-parameters" field="headerParameters" usage="optional" map-as="_HeaderParameters_Type"/>
    <structure field="credentials" usage="optional" map-as="org.openspcoop2.message.context.Credentials"/>
    <value name="web-context" field="webContext" usage="optional"/>
    <value name="request-uri" field="requestUri" usage="optional"/>
    <value name="request-type" field="requestType" usage="optional"/>
    <value name="source" field="source" usage="optional"/>
    <value name="protocol-name" field="protocolName" usage="optional"/>
    <value name="protocol-web-context" field="protocolWebContext" usage="optional"/>
    <value name="function" field="function" usage="optional"/>
    <value name="function-parameters" field="functionParameters" usage="optional"/>
    <value name="interface-name" field="interfaceName" usage="optional"/>
  </mapping>
  <mapping name="transport-response-context" class="org.openspcoop2.message.context.TransportResponseContext">
    <namespace uri="http://www.openspcoop2.org/message/context" default="elements"/>
    <structure name="header-parameters" field="headerParameters" usage="optional" map-as="_HeaderParameters_Type"/>
    <value name="transport-code" field="transportCode" usage="optional"/>
    <value name="content-length" field="contentLength" usage="optional"/>
    <value name="error" field="error" usage="optional"/>
  </mapping>
  <mapping name="forced-response" class="org.openspcoop2.message.context.ForcedResponse">
    <namespace uri="http://www.openspcoop2.org/message/context" default="elements"/>
    <value name="response-code" field="responseCode" usage="optional"/>
    <value name="empty-response" field="emptyResponse" usage="optional"/>
    <structure field="responseMessage" usage="optional" map-as="org.openspcoop2.message.context.ForcedResponseMessage"/>
  </mapping>
  <mapping name="response-message" class="org.openspcoop2.message.context.ForcedResponseMessage">
    <namespace uri="http://www.openspcoop2.org/message/context" default="elements"/>
    <value name="content" field="content" usage="optional"/>
    <value name="content-type" field="contentType" usage="optional"/>
    <value name="response-code" field="responseCode" usage="optional"/>
    <structure name="header-parameters" field="headerParameters" usage="optional" map-as="_HeaderParameters_Type"/>
  </mapping>
  <mapping name="serialized-context" class="org.openspcoop2.message.context.SerializedContext">
    <namespace uri="http://www.openspcoop2.org/message/context" default="elements"/>
    <collection field="property" create-type="java.util.ArrayList">
      <structure map-as="org.openspcoop2.message.context.SerializedParameter"/>
    </collection>
  </mapping>
  <mapping name="content-type-parameters" class="org.openspcoop2.message.context.ContentTypeParameters">
    <namespace uri="http://www.openspcoop2.org/message/context" default="elements"/>
    <collection field="parameter" create-type="java.util.ArrayList">
      <structure name="parameter" map-as="_StringParameter_Type"/>
    </collection>
  </mapping>
  <mapping name="content-length" class="org.openspcoop2.message.context.ContentLength">
    <namespace uri="http://www.openspcoop2.org/message/context" default="elements"/>
    <value name="outgoing-size" field="outgoingSize" usage="optional"/>
    <value name="incoming-size" field="incomingSize" usage="optional"/>
    <value name="incoming-size-forced" field="incomingSizeForced" usage="optional"/>
  </mapping>
  <mapping name="url-parameters" class="org.openspcoop2.message.context.UrlParameters">
    <namespace uri="http://www.openspcoop2.org/message/context" default="elements"/>
    <collection field="urlParameter" create-type="java.util.ArrayList">
      <structure name="url-parameter" map-as="_StringParameter_Type"/>
    </collection>
  </mapping>
  <mapping name="header-parameters" class="org.openspcoop2.message.context.HeaderParameters" abstract="false">
    <namespace uri="http://www.openspcoop2.org/message/context" default="elements"/>
    <collection field="headerParameter" create-type="java.util.ArrayList">
      <structure name="header-parameter" map-as="_StringParameter_Type"/>
    </collection>
  </mapping>

  <mapping type-name="_HeaderParameters_Type" class="org.openspcoop2.message.context.HeaderParameters" abstract="true">
    <namespace uri="http://www.openspcoop2.org/message/context" default="elements"/>
    <collection field="headerParameter" create-type="java.util.ArrayList">
      <structure name="header-parameter" map-as="_StringParameter_Type"/>
    </collection>
  </mapping>
  <mapping name="url-parameter" class="org.openspcoop2.message.context.StringParameter" abstract="false">
    <namespace uri="http://www.openspcoop2.org/message/context" default="elements"/>
    <value field="base" usage="required" style="text"/>
    <value name="nome" field="nome" usage="required" style="attribute"/>
  </mapping>

  <mapping type-name="_StringParameter_Type" class="org.openspcoop2.message.context.StringParameter" abstract="true">
    <namespace uri="http://www.openspcoop2.org/message/context" default="elements"/>
    <value field="base" usage="required" style="text"/>
    <value name="nome" field="nome" usage="required" style="attribute"/>
  </mapping>
  <mapping name="property" class="org.openspcoop2.message.context.SerializedParameter">
    <namespace uri="http://www.openspcoop2.org/message/context" default="elements"/>
    <value field="base" usage="required" style="text"/>
    <value name="nome" field="nome" usage="required" style="attribute"/>
    <value name="classe" field="classe" usage="required" style="attribute"/>
  </mapping>
  <mapping name="credentials" class="org.openspcoop2.message.context.Credentials">
    <namespace uri="http://www.openspcoop2.org/message/context" default="elements"/>
    <value name="principal" field="principal" usage="optional"/>
    <value name="subject" field="subject" usage="optional"/>
    <value name="username" field="username" usage="optional"/>
    <value name="password" field="password" usage="optional"/>
  </mapping>
</binding>
