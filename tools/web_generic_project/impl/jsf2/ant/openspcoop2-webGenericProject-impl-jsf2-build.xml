<project name="openspcoop2-compile-webGenericProject-impl-jsf2">
    <description>
        Produzione dei jar relativi al package org.openspcoop2.generic_project.web.impl.jsf2
    </description>

	<import file="${openspcoop2}/ant/commons/svn-build.xml" />
	<import file="${openspcoop2}/ant/commons/compile-build.xml" />
	<import file="${openspcoop2}/ant/commons/api-build.xml" />
	
	<!-- estensione di ant (if .... ) -->
	<taskdef resource="net/sf/antcontrib/antcontrib.properties">
          <classpath>
              <pathelement location="${required_lib}/ant/ant-contrib-1.0b3.jar"/>
          </classpath>
	</taskdef>
	

	
	<!-- Compilazione -->
	<target name="compile_openspcoop2_webGenericProjectImplJsf2" description="compila la libreria">

		<if>
			<istrue value="${buildOpenSPCoop2WebGenericProjectImplJsf2}"/>
			<then>
		
				<!-- Info svn -->
				<runtarget target="checksvn" />
				
				<!-- webGenericProjectImplJsf2 -->
				<var name="packagesCompiled" value="org.openspcoop2.generic_project.web.impl.jsf2" />
				<var name="packagesCompiled" value="${packagesCompiled},org.openspcoop2.generic_project.web.impl.jsf2.bean" />
				<var name="packagesCompiled" value="${packagesCompiled},org.openspcoop2.generic_project.web.impl.jsf2.converter" />
				<var name="packagesCompiled" value="${packagesCompiled},org.openspcoop2.generic_project.web.impl.jsf2.dao" />
				<var name="packagesCompiled" value="${packagesCompiled},org.openspcoop2.generic_project.web.impl.jsf2.datamodel" />
				<var name="packagesCompiled" value="${packagesCompiled},org.openspcoop2.generic_project.web.impl.jsf2.exception" />
				<var name="packagesCompiled" value="${packagesCompiled},org.openspcoop2.generic_project.web.impl.jsf2.form" />
				<var name="packagesCompiled" value="${packagesCompiled},org.openspcoop2.generic_project.web.impl.jsf2.input" />
				<var name="packagesCompiled" value="${packagesCompiled},org.openspcoop2.generic_project.web.impl.jsf2.input.factory.impl" />
				<var name="packagesCompiled" value="${packagesCompiled},org.openspcoop2.generic_project.web.impl.jsf2.input.impl" />
				<var name="packagesCompiled" value="${packagesCompiled},org.openspcoop2.generic_project.web.impl.jsf2.iservice" />
				<var name="packagesCompiled" value="${packagesCompiled},org.openspcoop2.generic_project.web.impl.jsf2.mbean" />
				<var name="packagesCompiled" value="${packagesCompiled},org.openspcoop2.generic_project.web.impl.jsf2.output.factory.impl" />
				<var name="packagesCompiled" value="${packagesCompiled},org.openspcoop2.generic_project.web.impl.jsf2.output.field" />
				<var name="packagesCompiled" value="${packagesCompiled},org.openspcoop2.generic_project.web.impl.jsf2.output.impl" />
				<var name="packagesCompiled" value="${packagesCompiled},org.openspcoop2.generic_project.web.impl.jsf2.table.factory.impl" />
				<var name="packagesCompiled" value="${packagesCompiled},org.openspcoop2.generic_project.web.impl.jsf2.table.impl" />
				<var name="packagesCompiled" value="${packagesCompiled},org.openspcoop2.generic_project.web.impl.jsf2.utils" />
				<antcall target="compile_src_openspcoop2">
					<param name="compile_src_openspcoop2.SVNINFO" value="${svnant.jar.presente}"/>
					<param name="compile_src_openspcoop2.SRC_DIR" value="${src_webGenericProjectImplJsf2}/"/>
					<param name="compile_src_openspcoop2.SVN_ID_NAME" value="web-generic-project-impl-jsf2"/>
					<param name="compile_src_openspcoop2.BUILD_INCLUDES" value="**/*.java"/>
					<param name="compile_src_openspcoop2.SRC_INCLUDES" value="*.xml"/>
					<param name="compile_src_openspcoop2.SRC_INCLUDES_2" value="*.properties"/>
					<param name="compile_src_openspcoop2.SRC_INCLUDES_3" value="META-INF/**"/>
					<param name="compile_src_openspcoop2.SRC_PREFIX" value=""/>
					<param name="compile_src_openspcoop2.PACKAGE" value="org.openspcoop2.generic_project.web.impl.jsf2"/>
					<param name="compile_src_openspcoop2.DIST_DIR_NAME" value="."/>
					<param name="compile_src_openspcoop2.JIBX_BINDING" value=""/>
					<param name="compile_src_openspcoop2.JIBX_CHECK" value=""/>
					<param name="dist" value="${dist_webGenericProjectImplJsf2}"/>
					<param name="build" value="${build_webGenericProjectImplJsf2}"/>
				</antcall>
								
				<!-- Jar Complessivo -->
				<!--
				<antcall target="jar_2_jar_openspcoop2">
					<param name="compile_src_openspcoop2.SVNINFO" value="${svnant.jar.presente}"/>
					<param name="compile_src_openspcoop2.SVN_ID_NAME" value="generic-project"/>
					<param name="compile_src_openspcoop2.DIST_DIR_NAME" value="webGenericProjectImplJsf2"/>
					<param name="dist" value="${dist_webGenericProjectImplJsf2}"/>
					<param name="build" value="${build_webGenericProjectImplJsf2}"/>
				</antcall>
				-->
				
				<!-- API -->
				<if>
					<istrue value="${buildAPI}"/>
					<then>
						<mkdir dir="${doc_webGenericProjectImplJsf2}"/>
					</then>
				</if>
				<antcall target="compile_api_openspcoop2">
					<param name="compile_src_openspcoop2.PACKAGES" value="${packagesCompiled}"/>
					<param name="compile_src_openspcoop2.DIST_DIR_NAME" value="web-generic-project-impl-jsf2"/>
					<param name="compile_src_openspcoop2.API_DIR_NAME" value="${doc_webGenericProjectImplJsf2}/api/web-generic-project-impl-jsf2"/>
					<param name="src" value="${src_webGenericProjectImplJsf2}"/>
					<param name="doc" value="${doc_webGenericProjectImplJsf2}"/>
					<param name="dist" value="${dist_webGenericProjectImplJsf2}"/>
				</antcall>
				
			</then>
		</if>
		
	</target>
	
</project>
