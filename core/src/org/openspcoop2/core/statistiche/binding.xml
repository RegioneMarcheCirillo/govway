<?xml version="1.0" encoding="UTF-8"?>
<binding force-classes="true">
  <mapping name="statistica-info" class="org.openspcoop2.core.statistiche.StatisticaInfo">
    <namespace uri="http://www.openspcoop2.org/core/statistiche" default="elements"/>
    <value name="tipo-statistica" field="tipoStatistica" usage="required" enum-value-method="toString"/>
    <value name="data-ultima-generazione" field="dataUltimaGenerazione" usage="required"/>
  </mapping>
