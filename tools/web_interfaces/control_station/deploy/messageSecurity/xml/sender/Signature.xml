<config id="messageSecurity.xml.sender.signature" label="XML Signature" xmlns="http://www.openspcoop2.org/core/mvc/properties"
	provider="org.openspcoop2.security.message.xml.SecurityProvider">

	 <compatibility not="false" and="false">
         <tags and="true">
                 <tag>REST</tag>
                 <tag>REQUEST</tag>
                 <tag>PD</tag>
         </tags>
         <tags and="true">
                 <tag>REST</tag>
                 <tag>RESPONSE</tag>
                 <tag>PA</tag>
         </tags>
	 </compatibility>
	
	<properties>
		<collection name="signaturePropRefId"/>
	</properties>
	
	<section label="Signature">
		
		<item type="hidden" name="securityEngine" value="xml">
			<property name="securityEngine" force="true"/>
		</item>
				
		<item type="hidden" name="signatureAction" value="Signature">
			<property name="action" force="true"/>
		</item>
		
		<subsection label="Algoritmi">
			
			<item type="select" name="signatureAlgorithm" label="Signature" default="http://www.w3.org/2001/04/xmldsig-more#rsa-sha256">
				<values>
					<value value="http://www.w3.org/2000/09/xmldsig#dsa-sha1" label="DSA-SHA1"/>
					<value value="http://www.w3.org/2009/xmldsig11#dsa-sha256" label="DSA-SHA256"/>
					<value value="http://www.w3.org/2000/09/xmldsig#rsa-sha1" label="RSA-SHA1"/>
					<value value="http://www.w3.org/2001/04/xmldsig-more#rsa-sha224" label="RSA-SHA224"/>
					<value value="http://www.w3.org/2001/04/xmldsig-more#rsa-sha256" label="RSA-SHA256"/>
					<value value="http://www.w3.org/2001/04/xmldsig-more#rsa-sha384" label="RSA-SHA384"/>
					<value value="http://www.w3.org/2001/04/xmldsig-more#rsa-sha512" label="RSA-SH512"/>
					<value value="http://www.w3.org/2001/04/xmldsig-more#ecdsa-sha1" label="ECDSA-SHA1"/>
					<value value="http://www.w3.org/2001/04/xmldsig-more#ecdsa-sha224" label="ECDSA-SHA224"/>
					<value value="http://www.w3.org/2001/04/xmldsig-more#ecdsa-sha256" label="ECDSA-SHA256"/>
					<value value="http://www.w3.org/2001/04/xmldsig-more#ecdsa-sha384" label="ECDSA-SHA384"/>
					<value value="http://www.w3.org/2001/04/xmldsig-more#ecdsa-sha512" label="ECDSA-SHA1512"/>
					<value value="http://www.w3.org/2000/09/xmldsig#hmac-sha1" label="HMAC-SHA1"/>
					<value value="http://www.w3.org/2001/04/xmldsig-more#hmac-sha224" label="HMAC-SHA224"/>
					<value value="http://www.w3.org/2001/04/xmldsig-more#hmac-sha256" label="HMAC-SHA256"/>
					<value value="http://www.w3.org/2001/04/xmldsig-more#hmac-sha384" label="HMAC-SHA384"/>
					<value value="http://www.w3.org/2001/04/xmldsig-more#hmac-sha512" label="HMAC-SHA512"/>
				</values>
				<property name="signatureAlgorithm" />
			</item>
			<item type="select" name="signatureDigestAlgorithm" label="Digest" default="http://www.w3.org/2001/04/xmlenc#sha256">
				<values>
					<value value="http://www.w3.org/2000/09/xmldsig#sha1" label="SHA1"/>
					<value value="http://www.w3.org/2001/04/xmldsig-more#sha224" label="SHA224"/>
					<value value="http://www.w3.org/2001/04/xmlenc#sha256" label="SHA256"/>
					<value value="http://www.w3.org/2000/09/xmldsig#sha384" label="SHA384"/>
					<value value="http://www.w3.org/2001/04/xmlenc#sha512" label="SHA512"/>
				</values>
				<property name="signatureDigestAlgorithm" />
			</item>
			<item type="select" name="signatureC14nAlgorithm" label="Canonicalization" required="false">
				<values>
					<value value="http://www.w3.org/2010/xml-c14n2" label=""/>
				</values>
				<property name="signatureC14nAlgorithm" />
			</item>

		</subsection>

		<subsection label="KeyStore">

			<item type="select" name="keystoreType" label="Tipo" default="jks">
				<values>
					<value value="jks" label="JKS"/>
					<value value="pkcs12" label="PKCS12"/>
					<!--  <value value="jceks" label="JCEKS"/> -->
				</values>
				<property name="rs.security.keystore.type" properties="signaturePropRefId" />
			</item>
			<item type="text" name="keystoreFile" label="File" required="true">
			      	<property name="rs.security.keystore.file" properties="signaturePropRefId" />
			</item>
			<item type="text" name="keystorePassword" label="Password" required="true">
			      	<property name="rs.security.keystore.password" properties="signaturePropRefId" />
			</item>
			<item type="text" name="keystorePrivateKeyAlias" label="Alias Chiave Privata" required="true">
			      	<property name="rs.security.keystore.alias" properties="signaturePropRefId" />
			</item>
			<item type="text" name="keystorePrivateKeyPassword" label="Password Chiave Privata" required="true">
			      	<property name="rs.security.key.password" properties="signaturePropRefId" />
			</item>

		</subsection>

	</section>

</config>



