	<managed-bean>
		<managed-bean-name>utentiBean</managed-bean-name>
		<managed-bean-class>org.openspcoop2.web.monitor.core.mbean.UtentiBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>service</property-name>
