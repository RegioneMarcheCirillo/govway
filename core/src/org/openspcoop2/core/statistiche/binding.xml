    <namespace uri="http://www.openspcoop2.org/core/statistiche" default="elements"/>
    <structure name="statistica-base" field="statisticaBase" usage="required" map-as="_Statistica_Type"/>
    <collection field="statisticaOrariaContenuti" create-type="java.util.ArrayList">
      <structure name="statistica-oraria-contenuti" map-as="_StatisticaContenuti_Type"/>
    </collection>
  </mapping>
  <mapping name="statistica-giornaliera" class="org.openspcoop2.core.statistiche.StatisticaGiornaliera">
    <namespace uri="http://www.openspcoop2.org/core/statistiche" default="elements"/>
