<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name>Esempio</display-name>
  <description>Semplice applicazione di esempio</description>

	<listener>
		<listener-class>org.openspcoop2.utils.cache.test.ExampleServletInitListener</listener-class>
	</listener>


	<servlet>
    	<servlet-name>Example</servlet-name>
