 <tag>
  <description>Renderizza un fieldset collapsible
	</description>
  <name>fieldset</name>
  <tag-class>taglibFieldSet.xhtml</tag-class>
  <body-content>tagdependent</body-content>
  <attribute>
   <name>rendered</name>
   <rtexprvalue>false</rtexprvalue>
   <type>Boolean</type>
  </attribute>
  <attribute>
   <description>Se renderizzare il pannello inizialmente chiuso. Questo tag viene valutato solo se presente il tag collapsible</description>
   <name>closed</name>
   <rtexprvalue>false</rtexprvalue>
   <type>Boolean</type>
  </attribute>
  <attribute>
   <description>Se il fieldset deve essere richiudibile o no. 
Default value: true</description>
   <name>collapsible</name>
   <rtexprvalue>false</rtexprvalue>
   <type>Boolean</type>
  </attribute>
  <attribute>
   <description>La legend del fieldset</description>
   <name>legend</name>
   <rtexprvalue>false</rtexprvalue>
   <type>String</type>
  </attribute>
 </tag>
 <tag>
  <name>prop</name>
  <tag-class>taglibproperty.xhtml</tag-class>
  <body-content>tagdependent</body-content>
  <attribute>
   <name>label</name>
   <required>true</required>
   <rtexprvalue>false</rtexprvalue>
   <type>String</type>
  </attribute>
  <attribute>
   <description>Se required=true allora visualizza un * accanto al label, in modo da evidenziare il fatto che questa proprieta e' richiesta</description>
   <name>required</name>
   <rtexprvalue>false</rtexprvalue>
   <type>Boolean</type>
  </attribute>
  <attribute>
   <description>Visualizza una stringa informativa proprio sotto l'elemento, serve per fornire maggiori informazioni su questa proprieta all'utente</description>
   <name>note</name>
   <rtexprvalue>false</rtexprvalue>
   <type>String</type>
  </attribute>
  <attribute>
   <description>Nome della classe css.</description>
   <name>styleClass</name>
   <rtexprvalue>false</rtexprvalue>
   <type>String</type>
  </attribute>
 </tag>
</taglib>
