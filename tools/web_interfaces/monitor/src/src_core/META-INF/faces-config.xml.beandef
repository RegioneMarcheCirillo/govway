			<value>#{userService}</value>
		</managed-property>
		<managed-property>
			<property-name>search</property-name>
			<property-class>org.openspcoop2.web.monitor.core.bean.UtentiSearchForm</property-class>
			<value>#{searchFormUtenti}</value>
		</managed-property>
	</managed-bean>
