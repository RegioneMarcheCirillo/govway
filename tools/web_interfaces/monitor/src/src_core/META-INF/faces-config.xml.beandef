	<managed-bean>
		<managed-bean-name>statusBean</managed-bean-name>
		<managed-bean-class>org.openspcoop2.web.monitor.core.mbean.StatusBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
