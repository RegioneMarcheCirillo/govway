<?xml version="1.0" encoding="iso-8859-1"?>

<project name="soapBlocking_LocalEnv" basedir=".">

      <!-- root di OpenSPCoop -->
      <property name="openspcoop2" location="../../../../../"/>
      <property name="openspcoop2.dist" location="${openspcoop2}/core/dist"/>
      
      <property name="required_lib_soapBlocking" location="${openspcoop2}/lib/"/>
      <property name="required_lib" value="${required_lib_soapBlocking}"/>
      <property name="openspcoop2.lib" location="${required_lib_soapBlocking}"/>

      <!-- Build-Options -->
      <property name="buildAsRelease" value="false"/>
      <property name="buildSoapBlocking" value="true"/>
      <property name="buildAPI" value="true"/>

       <!-- Specifica se inserire le informazioni di debug nelle classi -->
      <property name="debug" value="true" />
      <property name="printServerInfo" value="false" />

</project>
